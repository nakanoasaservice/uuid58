{
  "version": "1.0.4",
  "name": "@nakanoaas/uuid58",
  "license": "MIT",
  "exports": "./index.ts",
  "tasks": {
    "build": "deno run --allow-env --allow-read --allow-write --allow-run='npm' build.ts",
    "benchmark": "deno bench benchmark.ts"
  },
  "publish": {
    "include": [
      "alphabet.ts",
      "decode.ts",
      "encode.ts",
      "index.ts",
      "LICENSE",
      "README.md",
      "uuid58.ts"
    ]
